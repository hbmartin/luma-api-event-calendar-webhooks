{
	"name": "luma-api-event-calendar-webhooks",
	"private": false,
	"version": "0.0.1",
	"description": "A TypeScript library for the Luma calendar and event APIs with support for incoming webhooks.",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.cts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist/index.cjs",
		"dist/index.cjs.map",
		"dist/index.js",
		"dist/index.js.map",
		"dist/index.d.ts",
		"dist/index.d.cts",
		"README.md",
		"LICENSE",
		"package.json"
	],
	"sideEffects": false,
	"scripts": {
		"clean": "rimraf dist",
		"build": "pnpm clean && tsup --config tsup.config.ts",
		"typecheck": "tsc --project tsconfig.json --noEmit",
		"format": "prettier --write \"{src,test,demo/src}/**/*.{ts,tsx}\"",
		"lint": "eslint src",
		"publint": "publint",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"types:lint": "npm_config_cache=.npm-cache attw --pack .",
		"fta": "fta src"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hbmartin/luma-event-api-with-zapier-webhooks.git"
	},
	"keywords": [
		"luma",
		"calendar",
		"event",
		"api",
		"zapier",
		"webhooks",
		"sdk"
	],
	"author": {
		"name": "Harold Martin",
		"email": "harold.martin@gmail.com",
		"url": "https://www.linkedin.com/in/harold-martin-98526971/"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/hbmartin/luma-event-api-with-zapier-webhooks/issues"
	},
	"homepage": "https://github.com/hbmartin/luma-event-api-with-zapier-webhooks",
	"devDependencies": {
		"@arethetypeswrong/cli": "^0.18.2",
		"@eslint/eslintrc": "^3.3.3",
		"@types/node": "^24",
		"@vitest/coverage-v8": "^4.0.16",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-code-complete": "^1.3.0",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-jest": "^29.5.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-package-json": "^0.87.1",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-promise": "^7.2.1",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^7.0.1",
		"eslint-plugin-react-perf": "^3.3.3",
		"eslint-plugin-react-refresh": "^0.4.26",
		"eslint-plugin-security": "^3.0.1",
		"eslint-plugin-sonarjs": "^3.0.5",
		"eslint-plugin-unicorn": "^62.0.0",
		"eslint-plugin-unused-imports": "^4.3.0",
		"fta-cli": "^3.0.0",
		"jiti": "^2.6.1",
		"jsonc-eslint-parser": "^2.4.2",
		"prettier": "^3.7.4",
		"publint": "^0.3.16",
		"rimraf": "^6.1.2",
		"ts-node": "^10.9.2",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.50.0",
		"vite": "^7.3.0",
		"vitest": "^4.0.16",
		"zod": "^4.3.4"
	},
	"peerDependencies": {
		"zod": "^4"
	},
	"engines": {
		"node": ">=22"
	},
	"resolutions": {
		"@types/node": "^24"
	},
	"packageManager": "pnpm@10.27.0",
	"publishConfig": {
		"access": "public"
	}
}
